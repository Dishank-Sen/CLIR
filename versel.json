{
    "functions": {
      "api/server.js": {
        "runtime": "nodejs18.x"
      }
    },
    "routes": [
      { "src": "/api/(.*)", "dest": "/api/server.js" },
      { "src": "/clubs", "dest": "/api/server.js" },
      { "src": "/(.*)", "dest": "/public/$1" },
      { "src": "/", "dest": "/public/index.html" }
    ]
}
  