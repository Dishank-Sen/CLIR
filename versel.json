{
    "builds": [
      { "src": "api/server.js", "use": "@vercel/node" }
    ],
    "routes": [
      { "src": "/api/(.*)", "dest": "/api/server.js" },
      { "src": "/(.*)", "dest": "/index.html" }
    ],
    "rewrites": [
      { "source": "/api/(.*)", "destination": "/api/server.js" },
      { "source": "/(.*)", "destination": "/index.html" }
    ]
  }
  